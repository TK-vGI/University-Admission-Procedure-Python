def calculate_mean_score(num: list) -> float:
    return sum(num) / len(num)

def print_output(mean_score: float) -> None:
    if mean_score >= 60.0:
        print("Congratulations, you are accepted!")
    else:
        print("We regret to inform you that we will not be able to offer you admission.")


def main():
    # Take input from user
    num = []
    for _ in range(3):
        num.append(int(input()))

    # Calculate and print mean score
    mean_score = calculate_mean_score(num)
    print(f"{mean_score}")
    # print(f"{mean_score:.1f}") # Formatted to 1 decimal digit

    # Output result according to score
    print_output(mean_score)


if __name__ == '__main__':
    main()